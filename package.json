{
  "name": "create-a-mondrian",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "bankai build index.js --base https://www.finnpauls.de/create-a-mondrian/ && gh-pages -d dist",
    "create": "choo-scaffold",
    "inspect": "bankai inspect index.js",
    "start": "bankai start index.js",
    "test": "standard && npm run test-deps",
    "test-deps": "dependency-check . && dependency-check . --extra --no-dev -i tachyons"
  },
  "dependencies": {
    "choo": "^6.13.1",
    "sheetify": "^7.3.3",
    "tachyons": "^4.11.1"
  },
  "devDependencies": {
    "bankai": "^9.15.0",
    "choo-devtools": "^2.5.1",
    "choo-scaffold": "^1.2.0",
    "dependency-check": "^3.2.1",
    "gh-pages": "^2.0.1",
    "standard": "^12.0.1"
  }
}
